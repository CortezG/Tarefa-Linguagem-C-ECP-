#include <stdio.h>

int main(){
	int tam, i, j, k=0, v[3][5], v2[3][5];
	for(i = 0 ; i < 3; i += 1){
		for (j = 0 ; j < 5 ; j+=1){
			k+=1;
			printf("o valor em v[%d][%d] serah: ", i+1, j+1);
			scanf("%d", &v[i][j]);
		//	printf("v[%d][%d]", i, j);
		}
	}
	for(i = 0 ; i < 3; i += 1){
		for (j = 0 ; j < 5 ; j+=1){
			k+=1;
			printf("o valor em v2[%d][%d] serah: ", i+1, j+1);
			scanf("%d", &v2[i][j]);
		//	printf("v[%d][%d]", i, j);
		}
		
	}
	for(i = 0 ; i < 3; i += 1){
		printf("\n");
		for (j = 0 ; j < 5 ; j+=1){
			
			printf("%d  ", v[i][j]+v2[i][j]);
		}
	}
}
